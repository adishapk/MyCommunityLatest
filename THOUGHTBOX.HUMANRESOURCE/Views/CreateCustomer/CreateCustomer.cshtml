                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fab fa-intercom"></i></span>
                                        </div>
                                        <select class="form-control has-feedback-left" id="frm_custtype" required>
                                            <option value="">--Choose an Option--</option>
                                            <option value="Associate">Associate</option>
                                            <option value="Client">Client</option>
                                            <option value="Seller">Seller</option>

                                        </select>
                                    </div>

                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">
                                    <label>Paid Amount</label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-money-check-alt"></i></span>
                                        </div>
                                        <input type="text" id="frm_paidamnt" class="form-control has-feedback-lef" placeholder="Paid Amount" maxlength="10">
                                    </div>

                                    <!-- /.input group -->
                                </div>


                            </div>



                            <div class="row">


                                <div class="col-6 field item form-group">
                                    <label>Marketing Executive<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-house-user"></i></span>
                                        </div>
                                        <select class="form-control has-feedback-left" id="frm_empployeeid" required>
                                        </select>
                                    </div>

                                    <!-- /.input group -->
                                </div>

                            


                            </div>




                            <br /><br />
                            <div class="col-2">
                                <button type="button" id="btn_submit" class="btn btn-primary btn-block" value="Submit" onclick="createcustomerinsert()">Submit</button>
                            </div>
                        </form>
                    </div>

                    <!-- /.card-body -->
                </div>
                <!-- /.card -->

            </div>
