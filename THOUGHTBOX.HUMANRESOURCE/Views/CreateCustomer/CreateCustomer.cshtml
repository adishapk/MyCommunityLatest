                    </div>
                    <div class="card-body">
                        <form id="RegisterValidation">

                            <div class="row">
                                <div class="col-6 field item form-group">
                                    <label>Customer Name<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="far fa-file-image"></i></span>
                                        </div>
                                        <input type="text" id="frm_customername" class="form-control has-feedback-lef" placeholder="Customer Name" maxlength="75" required>
                                    </div>

                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">
                                    <label>Street Address</label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="far fa-address-card"></i></span>
                                        </div>
                                        <input type="text" id="frm_strtaddress" class="form-control has-feedback-lef" placeholder="Street Address" maxlength="200">
                                        <input type="hidden" id="customer_id" />
                                    </div>

                                    <!-- /.input group -->
                                </div>

                            </div>

                            <div class="row">
                                <div class="col-6 field item form-group">
                                    <label>City</label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="far fa-building"></i></span>
