                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">
                                    <label>State</label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-flag-usa"></i></span>
                                        </div>
                                        <input type="text" id="frm_state" class="form-control has-feedback-lef" placeholder="State" maxlength="50">
                                    </div>

                                    <!-- /.input group -->
                                </div>


                            </div>

                            <div class="row">
                                <div class="col-6 field item form-group">
                                    <label>Phone Number</label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-phone-volume"></i></span>
                                        </div>
                                        <input type="text" id="frm_phnumber" class="form-control has-feedback-lef" placeholder="Phone Number" maxlength="25">
                                    </div>

                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">
                                    <label>Email Address</label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="far fa-envelope"></i></span>
                                        </div>
                                        <input type="email" id="frm_email" class="form-control has-feedback-lef" placeholder="Email Address" maxlength="100">
                                    </div>

                                    <!-- /.input group -->
                                </div>


                            </div>

                            <div class="row">

                                <div class="col-6 field item form-group">
                                    <label>Contact Person (CP)</label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-user-clock"></i></span>
                                        </div>
                                        <input type="text" id="frm_contactpersn" class="form-control has-feedback-lef" placeholder="Contact Person (CP)" maxlength="100">
                                    </div>

                                    <!-- /.input group -->
                                </div>



                                <div class="col-6 field item form-group">
                                    <label>(CP) Designation</label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-map-marked"></i></span>
                                        </div>
                                        <input type="text" id="frm_cpdesigntn" class="form-control has-feedback-lef" placeholder="(CP) Designation" maxlength="100">
                                       
                                    </div>

                                    <!-- /.input group -->
                                </div>




                            </div>

                            <div class="row">

                                <div class="col-6 field item form-group">
                                    <label>Status<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-thermometer-half"></i></span>
                                        </div>
                                        <select class="form-control has-feedback-left" id="frm_cpstatus" required="required">
                                            <option value="">--Choose an Option--</option>
                                            <option value="Active" selected>Active</option>
                                            <option value="Inactive">In Active</option>
                                        </select>
                                    </div>

                                    <!-- /.input group -->
                                </div>


                                <div class="col-6 field item form-group">
                                    <label>Paid Status<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-battery-half"></i></span>
                                        </div>
                                        <select class="form-control has-feedback-left" id="frm_paidstatus" required>
                                            <option value="">--Choose an Option--</option>
                                            <option value="Paid">Paid</option>
                                            <option value="Notpaid">Not Paid</option>

                                        </select>
                                    </div>

                                    <!-- /.input group -->
                                </div>



                            </div>


