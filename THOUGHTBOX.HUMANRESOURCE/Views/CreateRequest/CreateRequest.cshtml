<!-- Main content -->
<section class="content">
    <div class="container-fluid">

        <!-- /.row -->
        <!-- Main row -->
        <div class="row">
            <div class="col-md-12">

                <div class="card card-danger">
                    <div class="card-header">
                        <h3 class="card-title">New Request Details can create here</h3>
                    </div>
                    <div class="card-body">
                        <form id="RegisterValidation">

                            <div class="row">
                                <div class="col-6 field item form-group">
                                    <label>Request Title<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-prescription-bottle"></i></span>
                                        </div>
                                        <input type="text" id="frm_reqsttiltle" class="form-control has-feedback-lef" placeholder="Request Title" maxlength="75" required>
                                    </div>

                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">
                                    <label>Request Date<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-table"></i></span>
                                        </div>
                                        <input type="date" id="frm_reqstdate" class="form-control has-feedback-lef" placeholder="Request Date" maxlength="20" required>
                                        <input type="hidden" id="request_id" />
                                    </div>

                                    <!-- /.input group -->
                                </div>

                            </div>

                            <div class="row">

                                <div class="col-6 field item form-group">
                                    <label>Supporting Document</label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="far fa-file-alt"></i></span>
                                        </div>
                                        <input id="frm_imagephotograph" type="file" name="frm_imagephotograph[]" class="file" data-show-upload="true" data-show-caption="true">
                                        <input id="hiddenimage" type="hidden" />

                                        <span id="spanimage"></span>
                                    </div>

                                    <!-- /.input group -->
                                </div>


                                <div class="col-6 field item form-group">
                                    <label>Priority Type<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fab fa-product-hunt"></i></span>
                                        </div>
                                        <select class="form-control has-feedback-left" id="frm_priortytype" required="required">
                                            <option value="">Select</option>
                                            <option value="Urgent">Urgent</option>
                                            <option value="Medium">Medium</option>
                                            <option value="Normal">Normal</option>
                                        </select>
                                    </div>

                                    <!-- /.input group -->
                                </div>


                            </div>

                            <div class="row">

                                <div class="col-6 field item form-group">
                                    <label>Document Number<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-print"></i></span>
                                        </div>
                                        <input type="text" id="frm_reqstcode" class="form-control has-feedback-lef" placeholder="Document Number" maxlength="20" disabled required>
                                    </div>

                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">
                                    <label>Comments (If Any)</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="far fa-comment-dots"></i></span>
                                        </div>
                                        <input type="text" id="frm_reqstcomments" class="form-control has-feedback-lef" placeholder="Comments (If Any)" maxlength="300">
                                    </div>

                                    <!-- /.input group -->
                                </div>

                            </div>

                            <div class="row">
                               
                                <div class="col-6 field item form-group">
                                    <label>Request Details<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fas fa-asterisk"></i></span>
                                        </div>
                                        <textarea rows="5" cols="150" placeholder="Request Details" id="frm_rqstdetails" class="form-control has-feedback-lef" maxlength="750" required></textarea>
                                    </div>

                                    <!-- /.input group -->
                                </div>

                            </div>


                            <br /><br />
                            <div class="col-2">
                                <button type="button" id="btn_submit" class="btn btn-primary btn-block" value="Submit" onclick="createrequestinsert()">Submit</button>
                            </div>
                        </form>
                    </div>

                    <!-- /.card-body -->
                </div>
                <!-- /.card -->

            </div>
            <!-- /.col (left) -->
          
            <!-- /.col (right) -->
        </div>
        <!-- /.row (main row) -->
    </div><!-- /.container-fluid -->
</section>
