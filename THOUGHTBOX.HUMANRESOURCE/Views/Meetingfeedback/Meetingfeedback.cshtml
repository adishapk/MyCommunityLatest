@model THOUGHTBOX.HUMANRESOURCE.Models.TempStore;
<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">Monthly Target Details of Employee</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">Dashboard v1</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->
<!-- Main content -->
<section class="content">
    <div class="container-fluid">

        <!-- /.row -->
        <!-- Main row -->
        <div class="row">
            <div class="col-md-12">

                <div class="card card-danger">
                    <div class="card-header">
                        <h3 class="card-title">New Division Name under a Department can create here</h3>
                    </div>
                    <div class="card-body">
                        <form id="RegisterValidation">
                            <div class="row">
                                <div class="col-6 field item form-group">

                                    <label>Reference No<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <input type="text" id="frm_refno" class="form-control has-feedback-lef" style="color:green;font-weight: bold;" placeholder="Reference No" maxlength="15" required>
                                        <input type="hidden" id="meeting_id">
                                        @Html.HiddenFor(m => m.custid, new { id = "RequestID" })
                                    </div>

                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">
                                    <label>Select Industry<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <select class="form-control has-feedback-left" id="frm_slctindustry" required="required">
                                        </select>
                                    </div>

                                    <!-- /.input group -->
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-6 field item form-group">
                                    <label>Customer Name<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <select class="form-control has-feedback-left" id="frm_custname" required="required">
                                        </select>
                                    </div>

                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">

                                    <label>Contact Person (CP)</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <input type="text" id="frm_contactpersn" class="form-control has-feedback-lef" style="color:red;font-weight: bold;" placeholder="Contact Person (CP)" maxlength="75" required>

                                    </div>

                                    <!-- /.input group -->
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-6 field item form-group">
                                    <label>(CP) Designation<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <input type="text" id="frm_cpdesigntn" class="form-control has-feedback-lef" style="color:red;font-weight: bold;" placeholder="(CP) Designation" maxlength="50" required>
                                    </div>

                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">

                                    <label>(CP) Details<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <input type="text" id="frm_cpadress" class="form-control has-feedback-lef" placeholder="(CP) Details" maxlength="300" required>

                                    </div>

                                    <!-- /.input group -->
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-6 field item form-group">
                                    <label>Appointment Taken Date<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <input type="date" id="frm_apmnttkndate" class="form-control has-feedback-lef" placeholder="Appointment Taken Date" maxlength="15" required>
                                    </div>

                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">

                                    <label>Meeting Date<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <input type="date" id="frm_meetingdate" class="form-control has-feedback-lef" placeholder="Meeting Date" maxlength="15" required>

                                    </div>

                                    <!-- /.input group -->
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-6 field item form-group">

                                    <label>Meeting Time<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <input type="time" id="frm_meetingtime" class="form-control has-feedback-lef" placeholder="Meeting Time" maxlength="15" required>

                                    </div>

                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">

                                    <label>Meeting Remarks<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <textarea rows="3" cols="150" placeholder="Meeting Remarks" id="frm_meetingremarks" class="form-control has-feedback-lef" maxlength="200"></textarea>

                                    </div>

                                    <!-- /.input group -->
                                </div>
                            </div>


                            <div class="row">
                                <div class="col-6 field item form-group">

                                    <label>Status<span class="required" style="color:red;font-weight: bold;font-size:large;">*</span></label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <select class="form-control has-feedback-left" id="frm_status" required="required">
                                            <option value="select">--Choose an Option--</option>
                                            <option value="Closed">Closed</option>
                                            <option value="Open">Open</option>
                                        </select>

                                    </div>

                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">

                                    <label>Any Document</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <input id="frm_anydocumnt" type="file" class="file" name="frm_anydocumnt[]" data-show-upload="true" data-show-caption="true">
                                        <input id="hiddenanydocument" type="hidden" />
                                        <span id="alert_document"></span>

                                    </div>

                                    <!-- /.input group -->
                                </div>
                            </div>


                            <div class="row">
                                <div class="col-6 field item form-group">

                                    <label>Next Meeting Date</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <input type="date" id="frm_nxtmtingdate" class="form-control has-feedback-lef" placeholder="Next Meeting Date" maxlength="15">

                                    </div>

                                    <!-- /.input group -->
                                </div>

                                <div class="col-6 field item form-group">

                                    <label>Next Meeting Time</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-book"></i></span>
                                        </div>
                                        <input type="time" id="frm_nxtmtingtime" class="form-control has-feedback-lef" placeholder="Next Meeting Time" maxlength="15">

                                    </div>

                                    <!-- /.input group -->
                                </div>
                            </div>

                            <br /><br />
                            <div class="col-4">
                                <button type="button" id="btn_submit" class="btn btn-primary btn-block" value="Submit" onclick="meetingfeedbackinsert()">Submit</button>
                            </div>
                        </form>
                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->

            </div>
            <!-- /.col (left) -->
            <div class="col-md-12">
                <div class="card card-primary">
                    <div class="card-header">
                        <h3 class="card-title">Created Divisions can View/Edit/Delete here</h3>
                    </div>
                    <div class="card-body">
                        <div class="card-body">
                            <table id="tbl1" class="table table-bordered table-striped">
                                <thead>
                                    <tr>

                                        <th>Reference No</th>
                                        <th>Meeting Date</th>
                                        <th>Meeting Time</th>
                                        <th>Edit</th>
                                        <th>Delete</th>
                                    </tr>
                                </thead>


                            </table>
                        </div>
                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
            <!-- /.col (right) -->
        </div>
        <!-- /.row (main row) -->
    </div><!-- /.container-fluid -->
</section>
<!-- /.content -->
<!-- page script -->
<script src="~/validator/jquery.min.js"></script>
<script src="~/validator/multifield.js"></script>
<script src="~/validator/validator.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script src="~/PScripts/JMeetingfeedback.js" type="text/javascript"></script>

